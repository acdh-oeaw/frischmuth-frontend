{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["./.nuxt/tsconfig.json", "@acdh-oeaw/tsconfig"],
	"compilerOptions": {
		"moduleResolution": "bundler",
		"allowJs": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": false,
		"paths": {
		  "@/*": ["./*"]
		}
	  }
  }